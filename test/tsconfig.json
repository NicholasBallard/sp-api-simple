{
    "ts-node": {
        "compilerOptions": {
            "target": "es6",
            "module": "CommonJS"
        }
    },
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "baseUrl": "./",
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "isolatedModules": false,
        "strict": false,
        "noImplicitAny": false
    },
    "exclude": [
        "../node_modules"
    ],
    "include": [
        "./**/*.ts"
    ]
}
